--Main

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(28.391758, 171.855148, -320.624664, 0, 0, -1, 0, 1, 0, 1, 0, 0)
local Window = Library.CreateLib("Ninja TycoonV4 [Hyago Clan]", "Sentinel")
local Tab = Window:NewTab("Misc")
local Section = Tab:NewSection("Exploits")
Section:NewKeybind("UI Toggle", "Set keybind to toggle UI on/off", Enum.KeyCode.F1, function()
	Library:ToggleUI()
end)
Section:NewToggle("Auto Click", "Toggle Auto Clicker on/off", function(state)
    if state then
        local hy_clicker = game:GetService("Workspace")["Zednov's Tycoon Kit"].Tycoons["Hyago Clan"].PurchasedObjects.Mine.Clicker.ClickDetector
        _G.autoclicker = true
      while wait() do
      if _G.autoclicker == true then
      fireclickdetector(hy_clicker)
      end
      end
    else
       _G.autoclicker = false
    end
end)
Section:NewToggle("Auto Take Money", "Toggle Auto Take Money on/off", function(statee)
    if statee then
        _G.automoney = true
        local hy_moneygiver = game:GetService("Workspace")["Zednov's Tycoon Kit"].Tycoons["Hyago Clan"].Essentials.Giver.TouchInterest
      while wait(1) do
      if _G.automoney == true then
      firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, hy_moneygiver.Parent, 0) --0 is touch
      wait()
      firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, hy_moneygiver.Parent, 1)
      end
      end
    else
       _G.automoney = false
    end
end)
Section:NewToggle("Healer[OP]", "You need Tree(Heal) on your tycoon.Its basically like\ngod mode its regen your health in very short time.Toggle Healer on/off", function(stateee)
    if stateee then
        _G.healer = true
      local healer = game:GetService("Workspace")["Zednov's Tycoon Kit"].Tycoons["Hyago Clan"].PurchasedObjects.Tree.HealingPad.TouchInterest
      while wait() do
      if _G.healer == true then
      firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, healer.Parent, 0) --0 is touch
      wait()
      firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, healer.Parent, 1)
      end
      end
    else
       _G.healer = false
    end
end)
Section:NewButton("Teleport Base", "Teleports to base.", function()
 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(79.5542679, 180.655075, -352.40506, 0, 0, 1, 0, -1, 0, 1, 0, -0)
end)
Section:NewSlider("WalkSpeed", "It changes your players walkspeed", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
Section:NewSlider("JumpPower", "It changes your players jump power", 500, 50, function(j) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = j
end)
Section:NewSlider("FOV", "It changes your players FOV", 120, 70, function(fw) -- 500 (MaxValue) | 0 (MinValue)
    cam.FieldOfView = fw
end)
Section:NewToggle("AntiAFK", "Toggle AntiAFK on/off", function(state1)
    if state1 then
        _G.antiafk = true
      while wait(1) do
      if _G.antiafk == true then
    game:GetService("Players").LocalPlayer.Idled:connect(function()
    vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    wait(1)
    vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    print("AntiAfk on")
    end)
    end
      end
    else
       _G.antiafk = false
       print("AntiAfk off")
    end
end)
Section:NewButton("[DONT RECCOMEND]Fire All TouchInterests", "Basically Fires All TouchInterests.Dont spam.", function()
      for _,v in pairs(workspace:GetDescendants()) do
if v:IsA("TouchTransmitter") then
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) --0 is touch
wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch
end
end
end)
