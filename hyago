local Tab3 = Window:NewTab("Remote Exploits")
local Section3 = Tab3:NewSection("Exploits")
Section3:NewToggle("Chidori Nagashi Spammer", "Charachter need to hold\nthe chidori nagashi if not then not work.", function(stateci)
    if stateci then
        _G.chispam = true
      while wait() do
      if _G.chispam == true then
      game:GetService("Players").LocalPlayer.Character:FindFirstChild("Chidori Nagashi").RemoteEvent:FireServer()
      end
      end
    else
       _G.chispam = false
    end
end)
